  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2019"
    ],
    "module": "commonjs",
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["bun-types"]
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts",
    "other/**/*.ts",
  ]
}
