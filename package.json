{
  "name": "hybrid-disk-cache",
  "version": "0.1.1",
  "description": "A hybrid disk cache utilize both the database and the file system.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "rm -rf dist && npm run build",
    "lint": "eslint src/*.ts test/*.ts",
    "test": "NODE_ENV=test mocha --require ts-node/register test/**/*.test.ts",
    "test-cov": "NODE_ENV=test nyc mocha test/*.test.ts"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "author": {
    "name": "Rakuraku Jyo",
    "email": "jyo.rakuraku@gmail.com"
  },
  "homepage": "https://github.com/rjyo/hybrid-disk-cache",
  "repository": {
    "type": "git",
    "url": "https://github.com/rjyo/hybrid-disk-cache.git"
  },
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^6.0.1",
    "fs-extra": "^9.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.0",
    "@types/chai": "^4.2.11",
    "@types/fs-extra": "^8.1.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1",
    "prettier": "^2.0.4",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "text-summary",
      "lcovonly",
      "html"
    ],
    "sourceMap": true
  },
  "keywords": [
    "disk-cache",
    "file-cache",
    "ttl",
    "sqlite3",
    "cache"
  ]
}
